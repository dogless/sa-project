.navbar.navbar-inverse.navbar-fixed-top{:role => "navigation"}
	.container
		.navbar-header
			%button.navbar-toggle{"data-target" => ".navbar-collapse", "data-toggle" => "collapse", :type => "button"}
				%span.sr-only Toggle navigation
				%span.icon-bar
				%span.icon-bar
				%span.icon-bar
			%a.navbar-brand{:href => "#"} Bing Bus
		.collapse.navbar-collapse
			%ul.nav.navbar-nav
				%li.active
					%a{:href => "#"} Search
			%form{:class => 'navbar-form navbar-left input-gmaps-autocomplete'}
				%input{:id => 'gmaps-input-address', :class => 'form-control search', :type => 'text', :placeholder => 'Destination', :autocomplete => 'off', 'aria-autocomplete' => 'list', 'aria-haspopup' => 'true'}

.mapbox
	#map
	= javascript_include_tag "index"
